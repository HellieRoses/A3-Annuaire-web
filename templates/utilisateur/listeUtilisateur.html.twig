{% extends 'base.html.twig' %}
{% block title %} Liste Utilisateur {% endblock %}
{% block stylesheets %}

    <link rel="stylesheet" type="text/css" href="{{ asset('css/utilisateurListStyle.css') }}"/>
{% endblock %}
{% block body %}
    <main>
    <div id="listUser">
    {% for user in users %}
        <a href="{{ path("profil", {code : user.code}) }}">
        {{ include('/utilisateur/utilisateur.html.twig', {'user' : user}) }}
        </a>
    {% endfor %}
    </div>
    </main>
{% endblock %}